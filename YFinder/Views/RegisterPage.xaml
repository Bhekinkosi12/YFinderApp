<ContentPage 
    xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="YFinder.Views.RegisterPage">
    <StackLayout>
        <TableView Intent="Form">
            <TableRoot>
                <TableSection Title="New User">
                    <EntryCell Label="Full Name" Placeholder="First Last" x:Name="fullName"/>
                    <EntryCell Label="Bio" Placeholder="Foodie who loves my friends and family" Keyboard="Text" x:Name="bio"/>
                    <EntryCell Label="Username" Placeholder="Xample123" Keyboard="Text" x:Name="userName"/>
                    <EntryCell Label="Email" Placeholder="xample@gmail.com" Keyboard="Email" x:Name="email"/>
                    <EntryCell Label="Zip" Placeholder="37211" Keyboard="Numeric" x:Name="zip"/>
                </TableSection>
            </TableRoot>
        </TableView>
       <Button Text="Register" Clicked="OnAdd" />
   </StackLayout>
</ContentPage>